{
    "name": "ritterg/sru-ao",
    "description": "Helper package to implement the SRU interface used by archivesonline.org",
    "license": "MIT",
    "keywords": [
        "SRU",
        "Archives Online",
        "archivesonline.org"
    ],
    "authors": [{
        "name": "Gerold Ritter",
        "email": "ritter@e-hist.ch",
        "homepage": "https://www.e-hist.ch",
        "role": "Developer"
    }],
    "require": {
        "php": "^7.2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Ritterg\\SruAo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ritterg\\SruAo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "stable"
}
